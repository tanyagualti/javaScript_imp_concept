<script type="text/javascript">
	
	
   let str="tanyagulati";
 let arr= Array.from(str);
 let count=0;
//length method count start with 1
 //array index is start with 0

 //for(i=0;i<=11-1=10;i++)
 for(let i=0;i<=arr.length-1;i++){
       count++;
       document.write(arr[i])
 }
 document.write(count);

